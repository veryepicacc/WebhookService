local ServerScriptService = game:GetService("ServerScriptService")
local WebhookService = require(ServerScriptService:WaitForChild("WebhookService"))

--You can customize the title, description, or color.
local EmbedData = {
	["content"] = "",
	["embeds"] = {{
		["title"] = "Title here",
		["description"] = "Text Here",
		["type"] = "rich",
		["color"] = tonumber(0xffffff),
	}}
}

WebhookService:Setup("https://discord.com/api/webhooks/Webhook")
WebhookService:PostWebhookFunc("Hi there!", "Text", nil) --Normal Text
WebhookService:PostWebhookFunc(nil, "Embed", EmbedData) --Embedding
